{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "/ProductsIMg/(.*)",
            "dest": "/public/ProductsIMg/$1"
        },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }